                    "404": {
                        "description": "Calle no encontrada",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Calle no encontrada"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/horarios": {
            "get": {
                "tags": [
                    "Horarios"
                ],
                "summary": "Listar todos los horarios por perfil",
                "operationId": "8559ff5b26b44077c955d57b832912a3",
                "parameters": [
                    {
                        "name": "perfil_id",
                        "in": "query",
                        "description": "El UUID del perfil para filtrar los horarios.",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "uuid"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Operación exitosa",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Horario"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/rutas/{ruta}/horarios": {
            "post": {
                "tags": [
                    "Horarios"
                ],
                "summary": "Crear un nuevo horario para una ruta",
                "description": "Crea un nuevo horario asociado a una ruta específica.",
                "operationId": "13b0c958559c1240015923317be8c95c",
                "parameters": [
                    {
                        "name": "ruta",
                        "in": "path",
                        "description": "ID de la ruta",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "uuid",
                            "example": "a1b2c3d4-e5f6-7890-abcd-1234567890ab"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "dia_semana",
                                    "hora_inicio"
                                ],
                                "properties": {
                                    "dia_semana": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "hora_inicio": {
                                        "type": "string",
                                        "format": "time",
                                        "example": "06:00:00"
                                    },
                                    "hora_fin": {
                                        "type": "string",
                                        "format": "time",
                                        "example": "08:00:00"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Horario creado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Horario"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validación fallida",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "El campo dia_semana es obligatorio."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
