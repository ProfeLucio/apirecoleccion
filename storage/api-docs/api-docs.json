                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "perfil_id"
                                ],
                                "properties": {
                                    "perfil_id": {
                                        "description": "ID del perfil propietario del recorrido.",
                                        "type": "string",
                                        "format": "uuid"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Recorrido finalizado."
                    },
                    "403": {
                        "description": "Acción no autorizada. El perfil no corresponde."
                    },
                    "404": {
                        "description": "Recorrido no encontrado."
                    }
                }
            }
        },
        "/api/vehiculos": {
            "get": {
                "tags": [
                    "Vehiculos"
                ],
                "summary": "Listar vehículos (filtrado opcional por perfil)",
                "description": "Devuelve una lista paginada de todos los vehículos, opcionalmente filtrada por perfil_id. Si perfil_id no se proporciona, lista todos los vehículos.",
                "operationId": "b912521b0e41dec60b1fd4d98a0e06fe",
                "parameters": [
                    {
                        "name": "perfil_id",
                        "in": "query",
                        "description": "UUID del perfil para filtrar sus vehículos",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "uuid",
                            "example": "a1b2c3d4-e5f6-7890-abcd-1234567890ab"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Listado de vehículos",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Vehiculo"
                                    }
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validación fallida (si el UUID es inválido)",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "El perfil_id proporcionado es inválido."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "Vehiculos"
                ],
                "summary": "Crear un nuevo vehículo",
                "description": "Crea un nuevo vehículo de recolección, requiere el perfil_id en el cuerpo de la petición.",
                "operationId": "20a2f18df869f0d5e7d5ddce18285c29",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "placa",
                                    "perfil_id"
                                ],
                                "properties": {
                                    "placa": {
                                        "type": "string",
                                        "example": "ABC123"
                                    },
                                    "marca": {
                                        "type": "string",
                                        "example": "Chevrolet"
                                    },
                                    "modelo": {
                                        "type": "string",
                                        "example": "2020"
                                    },
                                    "activo": {
                                        "type": "boolean",
                                        "example": true
                                    },
                                    "perfil_id": {
                                        "description": "ID del perfil al que pertenece el vehículo",
                                        "type": "string",
                                        "format": "uuid",
                                        "example": "e6a7b8c9-d0e1-2345-fghi-678901234567"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Vehículo creado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Vehiculo"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validación fallida",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "La placa ya existe o el perfil_id es inválido."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
