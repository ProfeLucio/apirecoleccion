        "/api/recorridos/{recorrido}/finalizar": {
            "post": {
                "tags": [
                    "Recorridos"
                ],
                "summary": "Finalizar un recorrido existente",
                "operationId": "137a4cd897a71f875627eda1291cf3c3",
                "parameters": [
                    {
                        "name": "recorrido",
                        "in": "path",
                        "description": "ID del recorrido a finalizar.",
